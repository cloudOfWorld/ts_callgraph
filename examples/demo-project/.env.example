# CallGraph Demo 环境配置

# 应用配置
NODE_ENV=development
PORT=3000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=password
DB_NAME=callgraph_demo

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASS=
REDIS_DB=0

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES=24h

# CORS配置
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# 日志配置
LOG_LEVEL=info
LOG_FILE=true
LOG_PATH=./logs/app.log

# 第三方API配置
API_TIMEOUT=10000
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100